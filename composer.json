{
    "name": "metaxy/sermons",
    "description": "Sermons API",
    "keywords": ["yii2"],
    "homepage": "http://ecg.berlin",
    "type": "project",
    "license": "GPLv3",
    "support": {
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "yiisoft/yii2": ">=2.0.5"
    },
    "require-dev": {
    },
    "config": {
        "process-timeout": 1800
    },
   
    "extra": {
        "yii\\composer\\Installer::post-install-cmd": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },"autoload": {
        "files": [
            "globals.php"
        ]
    }
}
